    "hijackresponse": "^5.0.0",
    "javascript-obfuscator": "^2.10.1",
    "jsbattle-engine": "^3.5.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "moleculer": "^0.14.12",
    "moleculer-db": "^0.8.11",
